	pickup pencircle xscaled 2.5wid yscaled .6wid rotated 100;
%	pickup pencircle scaled 7;
	x2=.45w;y2=2/16h;
	x3=.35w;
	top y4=.39h;
	y3=.5[y2,y4];
	rt x5=w;y5=.2[y2,y4];
	x4=.5[x3,x5];
	x6=.5w;bot y6=0.04;
	lft x7=0;y7=.4[y2,y4];
	x8=.5w;y8=.55h;
	rt x9=.81w;y9= 5/6h;
	x10=.72w;top y10=h;
%	x11=.41w;y11=16/18h;
	x11=.41w;y11=14/18h;
	x12=.8w;y12=-.5d;
	x13=.22w;y13=y12;
	x14=.5w;y14=y12;
	y16=.5[y11,y12];
	x16=.5[x11,x12];
	draw z2..z3..z4..z5..z6..z7..tension 1.2..z8..z9..z10
	& z10..z11---z16;
	pickup pencircle scaled .6wid;
	x15=.5[x13,x14];bot y15 = -d;
	draw  z16---z12..z15..{up}z13..z14{down}..z13;
	fill z13..z14..cycle;
