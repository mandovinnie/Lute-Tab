    x0=.5w;top y0=h;
    x12=.75w;y12=.83h;
    rt x1=w;y1=.8h;
    lft x4=0;y4=.5h;
    rt x6=w;y6=.32h;
    lft x10=0;y10=.2h;
    x11=0.3w;bot y11=0;

