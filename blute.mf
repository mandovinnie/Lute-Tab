%
def c_body(expr thick) =
% pickup pencircle xscaled 1.6 yscaled .4wid rotated 20;
 x0=1.05w;y0=.73h;
 rt x1=.97w;rt x4=w;
 y1=.62h;
 y2=.6h;
 y3=.4h;
 y4=.36h;
 x2=x3=.15w;
 x5=x6=.5w; top y5=.8h;bot y6=.2h;
 lft x7=0; y7=.5h;
% draw z0{dir 235}..z1;
% draw z1{dir 110}..z5..z2..z3..z6..{dir 65}z4;
 penpos0(1.wid, 25);
 penpos1(thick, 25);
 penpos2(thick, 135);
 penpos3(thick, 225);
 penpos4(thick, 315);
 penpos5(.5wid, 90);
 penpos6(.6wid, 270);
 
 penstroke z0e{dir 240}..z1e;
 penstroke z1e..z5e..z2e..z3e..z6e..{dir 65}z4e;
enddef;
