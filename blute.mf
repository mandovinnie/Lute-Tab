%
def do_v_bar = 
   pickup lute_pen;x3 = x4 = v_bar;top y3 = h;top y4 = 0;
   draw z3..z4;
enddef;
%
def do_box =
beginchar (0, em#, 1.0cap#, .3cap#); "em quad";
  pickup pencircle xscaled .3wid yscaled .3wid;
  x1=x4=0w;x2=x3=w;
  y1=y2=0h;y3=y4=h;
  x5=x1;y5=-d;
  x6=x2;y6=-d;
  draw z1..z2;draw z2..z3; draw z3..z4; draw z4..z1;
  draw z5..z6;
endchar;enddef;
