def do_g_clef =
beginchar (252, 1.6em#, 2.2cap#, 0); "G Clef";
   lft x0=.5w;top y0=h;
   rt x1=w;top y1 = h;
   x2=x1; y2=.64h;
   lft x3=0;y3=.36h;
   x4=.5w;bot y4=0;
   x5=x2;y5=y3;
   x6=x4;y6=.4h;
   x7=.48w;y7=.3h;

   penpos0(2.4 wid,-50);
   penpos1(2.4 wid,-40);
   penpos2(2.2 wid,-50);
   penpos3(2.2 wid,-10);
   penpos4(.8 wid,90);
   penpos5(1.6 wid, 180);
   penpos6(.4wid, 270);
   penpos7(.4wid, 330);
   penstroke z0e{dir 20}..z1e;
   penstroke z1e..z2e;
   penstroke z2e..z3e..z4e..z5e..z6e..z7e;
endchar;enddef;
