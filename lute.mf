 x$1=.5w;x$2=.9w;x$3=x$1;
 top y$1=the_top+0.03h;y$2=y$1 - 8/30h;
 y$3=y$1 - .5h;
 draw z$1..tension 1.4..{dir 250}z$2..z$3;
