def do_l_paren =
beginchar ("(", .42em#, 1.0cap#, .2cap#); "left parenthesis";
        pickup pencircle xscaled 1.0wid yscaled .8 wid rotated 40;
        rt x1=w;top y1=h;
        x4=x1;bot y4=-d;
        lft x2=0;y2=y1;
        x3=x2;y3=y4;
        draw z1..z2;
        draw z2..z3;
        draw z3..z4;
endchar;enddef;
%
def do_r_paren =
beginchar (")", .42em#, 1.0cap#, .2cap#); "left parenthesis";
        pickup pencircle xscaled 1.0wid yscaled .8 wid rotated 40;
        lft x1=0;top y1=h;
        x4=x1;bot y4=-d;
        rt x2=w;y2=y1;
        x3=x2;y3=y4;
        draw z1..z2;
        draw z2..z3;
        draw z3..z4;
endchar;enddef;
