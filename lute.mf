def do_half_clef =
beginchar (17, 1.6em#, 1.cap#, 2.4cap#); "old O Clef";
	pickup pencircle scaled 1.2wid;
	x2=.5w;top y2=0h;
	x4=x2;y4=1.h;
	lft x1=0w;y1=.5[y2,y4];
	rt x3=w;y3=y1;
	draw z4..z1..z2..z3..cycle;
	x20=.5w;y20=y1;
	x21=1.2w;y21=y1-h;
	x22=x21;y22=y1+h;
	erase fill z20..z21&z21..z22&z22..z20..cycle;
	x10=.2w;y10=1.25h;
	x11=.59w;y11=y10;
	x12=.5[x10,x11];y12=y10+.12h;
	x13=.42w;y13=.3h;
	x18=.5w;top y18=0h;
	x14=x11;y14=y11;
	x15=.55w;y15=-.68d;
	x16=x15-.014w;y16=-.88d;
	x17=.27w;bot y17=-d;
	penpos10(1.6wid,105);
	penpos12(1.6wid,90);
	penpos11(1.6wid,75);
	penpos14(.8wid, 0);
	penpos4(.8wid,0);
	penpos13(.8wid,0);
	penpos18(1.0wid,0);
	penpos15(1.35wid, 0);
	penpos16(1.4wid, -20);
	penpos17(1.6wid, -60);
	penstroke z10e..z12e..z11e;
	penstroke z14e..z4e..tension 1.13..z18e..tension 1.8..z16e..z17e;

endchar;enddef;
